<h2>Your Existing Posts</h2>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body<th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
        <td><%= link_to "Delete" ,post_path(post)%></td>
    <% end %>
  </tbody>
</table>

<br><h2><%= link_to "Add New Post",new_post_path%></h2>